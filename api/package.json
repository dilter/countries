{
  "name": "countries-api",
  "version": "0.8.0",
  "description": "Countries API",
  "main": "app.js",
  "scripts": {
    "start": "node --experimental-modules src/app.mjs"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.?(m)js?(x)",
      "**/?(*.)(spec|test).?(m)js?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "es2015",
            {
              "modules": false
            }
          ]
        ],
        "plugins": [
          [
            "transform-es2015-modules-commonjs",
            {
              "spec": true
            }
          ]
        ]
      }
    }
  },
  "author": "DÃ­lter Porto",
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.0",
    "body-parser": "^1.17.2",
    "camelcase": "^4.1.0",
    "camelcase-keys": "^4.2.0",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.15.3",
    "express-promise-router": "^1.1.1",
    "express-validator": "^5.0.3",
    "flat": "^4.0.0",
    "js-search": "^1.4.2",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "rest-facade": "^1.10.1",
    "swagger-ui-express": "^2.0.15",
    "uuid": "^3.2.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.6.0",
    "superagent": "^4.0.0-beta.5",
    "supertest": "^3.3.0"
  }
}
